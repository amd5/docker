# Base Image
FROM nginx:1.15

# Author
MAINTAINER c32 amd5@qq.com

# Add to docker and Unpacking
ADD http://c32.19aq.com/Linux/Docker/Nginx/default.conf /etc/nginx/conf.d/
ADD http://c32.19aq.com/Linux/Docker/Nginx/index1.html /usr/share/nginx/html/index1.html
ADD http://c32.19aq.com/Linux/Docker/Nginx/index1.php /usr/share/nginx/html/index1.php
RUN cd /usr/share/nginx/ && chmod 755 -R html
